<Application
    x:Class="HitTaiXiuLive.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <!--  Dùng pack URI: pack://application:,,,/ + đường dẫn trong project  -->
            <BitmapImage x:Key="ImgXIU" UriSource="pack://application:,,,/Assets/side/XIU.png" />
            <BitmapImage x:Key="ImgTAI" UriSource="pack://application:,,,/Assets/side/TAI.png" />
            <BitmapImage x:Key="ImgTHANG" UriSource="pack://application:,,,/Assets/kq/THANG.png" />
            <BitmapImage x:Key="ImgTHUA" UriSource="pack://application:,,,/Assets/kq/THUA.png" />
        </ResourceDictionary>
    </Application.Resources>
</Application>
